{
  "name": "security",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "scripts": {
    "save": "npm run encrypt && git add . && git commit -m 'update' && git push",
    "import-key": "npm run import-public && npm run import-private",
    "import-public": "gpg --import /Volumes/SarigamaKey/public.pgp",
    "import-private": "gpg --import /Volumes/SarigamaKey/private.pgp",
    "encrypt": "gpg --recipient 9D54203BE46A1CD4 --encrypt Sarigama.kdbx > Sarigama.kdbx.pgp",
    "postencrypt": "rm Sarigama.kdbx",
    "decrypt": "gpg -d Sarigama.kdbx.pgp > Sarigama.kdbx"
  },
  "author": "Lily Bucher <lily@lucidogen.io>"
}
